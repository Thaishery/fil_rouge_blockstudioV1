 <form method="post" action="{{ path('projet_delete_user', {'id': projet.id, 'login':app.user.login}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
     <input type="hidden" name="_method" value="DELETE">
     <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ projet.id) }}">

         <button class="btn btn-primary btn-lg btn-block">
             Supprimer
         </button>

 </form>
